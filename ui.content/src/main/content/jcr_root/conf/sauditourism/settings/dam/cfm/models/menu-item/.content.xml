<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    allowedPaths="[/content/entities(/.*)?, /content/dam(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:scaffolding="/conf/sauditourism/settings/dam/cfm/models/menu-item/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:primaryType="cq:PageContent"
        jcr:title="menu item"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default"
        status="enabled">
        <metadata jcr:primaryType="nt:unstructured"/>
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="21">
                        <title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Menu Item Label"
                            listOrder="21"
                            maxlength="255"
                            metaType="text-single"
                            name="title"
                            required="on"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </title>
                        <ctaLink
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Menu Item URL"
                            filter="hierarchy"
                            listOrder="8"
                            metaType="reference"
                            name="ctaLink"
                            nameSuffix="contentReference"
                            required="on"
                            renderReadOnly="false"
                            rootPath="/content/sauditourism"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </ctaLink>
                        <menuItemImage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            granite:class="s7-image-listener"
                            fieldLabel="Menu Item Image"
                            filter="hierarchy"
                            listOrder="8"
                            metaType="reference"
                            name="menuItemImage"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/dam"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                selector="s7-image-listener"
                                mvmaxitems="3"
                                showThumbnail="true"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </menuItemImage>
                        <menuItemImageFileReference
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            fieldLabel="Menu Item Image fileReference"
                            listOrder="21"
                            maxlength="255"
                            metaType="text-single"
                            name="s7menuItemImage"
                            renderReadOnly="true"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </menuItemImageFileReference>
                        <subMenuPage
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            fieldLabel="Sub Menu Pages"
                            filter="hierarchy"
                            listOrder="22"
                            metaType="reference"
                            name="subMenuPages"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/sauditourism"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference[]">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                mvmaxitems="6"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                            <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="dam/cfm/models/editor/components/contentreference"
                                name="subMenuPages"
                                renderReadOnly="false"
                                rootPath="/content/sauditourism">
                                <granite:data
                                    jcr:primaryType="nt:unstructured"
                                    mvmaxitems="6"
                                    thumbnail-validation="cfm.validation.thumbnail.show"/>
                            </field>
                        </subMenuPage>
                        <viewAllLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="View All Label"
                            listOrder="21"
                            maxlength="255"
                            metaType="text-single"
                            name="viewAllLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </viewAllLabel>
                        <viewAllLink
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="View all"
                            filter="hierarchy"
                            listOrder="21"
                            metaType="reference"
                            name="viewAllLink"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/sauditourism"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </viewAllLink>
                    </items>
                </content>
            </cq:dialog>
        </model>
    </jcr:content>
</jcr:root>
