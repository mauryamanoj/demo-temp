<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/sauditourism/settings/wcm/templates/story-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sauditourism/components/structure/story-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <menu
                jcr:lastModified="{Date}2023-09-17T07:28:46.598+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/menu"
                displayMode="multi"
                fragmentPath="/content/dam/sauditourism/cf/{language}/header/menu"
                variationName="master" />
            <page_banner
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/page-banner"

                editable="{Boolean}true"/>
            <breadcrumb
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/breadcrumb"

                editable="{Boolean}true"/>
            <about_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/about-section"
                editable="{Boolean}true"/>

            <information_list_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/information-list-widget"
                editable="{Boolean}true"/>
            <event_date_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/event-date-widget"
                editable="{Boolean}true"/>
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                editable="{Boolean}true">
            </responsivegrid>
            <rightsidebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                editable="{Boolean}true">
            </rightsidebar>
            <footer-block
                jcr:lastModified="{Date}2023-09-17T07:28:46.598+02:00"
                jcr:lastModifiedBy="admin"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/contentfragmentfooter"
                displayMode="multi"
                fragmentPath="/content/dam/sauditourism/cf/{language}/footer"
                variationName="master" />
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650" />
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200" />
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
