<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    allowedPaths="[/content/entities(/.*)?, /content/dam(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:scaffolding="/conf/sauditourism/settings/dam/cfm/models/destination/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:primaryType="cq:PageContent"
        jcr:title="Destination"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default"
        status="enabled">
        <metadata jcr:primaryType="nt:unstructured"/>
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="21">
                        <destination-title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Destination Title"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="title"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-title>
                        <destination-pageBannerTitle
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Destination Page Banner Title"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="pageBannerTitle"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-pageBannerTitle>
                        <destination-logo
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            granite:class="s7-image-listener"
                            fieldLabel="Logo Image"
                            fieldDescription="please upload only PNG Logos"
                            filter="hierarchy"
                            listOrder="2"
                            metaType="reference"
                            name="logo"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/dam"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                selector="s7-image-listener"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </destination-logo>
                        <destination-logo-file-reference
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                            fieldDescription="Fill this field only if type is 'logo'"
                            fieldLabel="Logo fileReference"
                            listOrder="3"
                            maxlength="255"
                            metaType="text-single"
                            name="s7logo"
                            renderReadOnly="true"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-logo-file-reference>
                        <destination-isTransparency
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="4"
                            metaType="boolean"
                            name="isTransparency"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Enable Logo transparency"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-isTransparency>
                        <destination-mix-logo-title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="5"
                            metaType="boolean"
                            name="replaceTitleWithLogo"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Replace Title With Logo"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-mix-logo-title>
                        <destination-tagLine
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Destination TagLine"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="tagline"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-tagLine>
                        <destination-sub-title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Destination Sub Title"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="subTitle"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </destination-sub-title>
                        <bannerImages
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            composite="{Boolean}true"
                            fieldLabel="Banner Images"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="images"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            showThumbnail="true"
                            valueType="string[]">
                            <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                name="./images">
                                <granite:data jcr:primaryType="nt:unstructured"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <typeAsset
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Type Image"
                                                name="type"
                                                renderReadOnly="false"
                                                showEmptyInReadOnly="true"
                                                valueType="string">
                                                <optionsmultifield jcr:primaryType="nt:unstructured">
                                                    <item0
                                                        jcr:primaryType="nt:unstructured"
                                                        fieldLabel="image"
                                                        fieldValue="image"/>
                                                    <item1
                                                        jcr:primaryType="nt:unstructured"
                                                        fieldLabel="video"
                                                        fieldValue="video"/>
                                                </optionsmultifield>
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="dam/cfm/admin/components/datasources/optionrendererenumeration"
                                                    variant="default"/>
                                                <granite:data jcr:primaryType="nt:unstructured"/>
                                            </typeAsset>
                                            <image
                                                granite:class="s7-image-listener"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="dam/cfm/models/editor/components/contentreference"
                                                fieldDescription="Fill this field only if type is 'image'"
                                                fieldLabel="Image"
                                                name="./image"
                                                renderReadOnly="false"
                                                rootPath="/content/dam"
                                                showThumbnail="true"
                                                validation="cfm.validation.contenttype.image">
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    mvmaxitems="3"
                                                    selector="s7-image-listener"
                                                    showThumbnail="true"
                                                    thumbnail-validation="cfm.validation.thumbnail.show"/>
                                            </image>
                                            <imageFileReference
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                fieldDescription="Fill this field only if type is 'image'"
                                                fieldLabel="Image fileReference"
                                                listOrder="21"
                                                maxlength="255"
                                                metaType="text-single"
                                                name="./s7image"
                                                renderReadOnly="true"
                                                showEmptyInReadOnly="true"
                                                valueType="string">
                                                <granite:data jcr:primaryType="nt:unstructured"/>
                                            </imageFileReference>
                                            <altImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                emptyText="Insert Alt Image"
                                                fieldDescription="Fill this field only if type is 'video'"
                                                fieldLabel="Alt Image"
                                                maxlength="255"
                                                name="./alt"
                                                renderReadOnly="false"
                                                showEmptyInReadOnly="true"
                                                valueType="string">
                                                <granite:data jcr:primaryType="nt:unstructured"/>
                                            </altImage>
                                            <video
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="dam/cfm/models/editor/components/contentreference"
                                                granite:class="s7-video-listener"
                                                fieldDescription="Fill this field only if type is 'video'"
                                                fieldLabel="Video"
                                                name="./video"
                                                rootPath="/content/dam"
                                                renderReadOnly="false"
                                                showThumbnail="true"
                                                validation="cfm.validation.contenttype.video">
                                                <granite:data
                                                    selector="s7-video-listener"
                                                    jcr:primaryType="nt:unstructured"
                                                    mvmaxitems="3"
                                                    showThumbnail="true"
                                                    thumbnail-validation="cfm.validation.thumbnail.show"/>
                                            </video>
                                            <videoFileReference
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                                fieldDescription="Fill this field only if type is 'video'"
                                                fieldLabel="Video fileReference"
                                                listOrder="21"
                                                maxlength="255"
                                                metaType="text-single"
                                                name="./s7video"
                                                renderReadOnly="true"
                                                showEmptyInReadOnly="true"
                                                valueType="string">
                                                <granite:data jcr:primaryType="nt:unstructured"/>
                                            </videoFileReference>
                                            <thumbnail
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Fill this field only if type is 'video'"
                                                fieldLabel="Video Thumbnail"
                                                name="./thumbnail"
                                                rootPath="/content/dam"/>
                                            <location
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This field presents the location related to the asset."
                                                fieldLabel="Image/video Location"
                                                name="./location"/>
                                        </items>
                                    </column>
                                </items>
                            </field>
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </bannerImages>
                        <isPopUpImageTransparent
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="22"
                            metaType="boolean"
                            name="isPopUpImageTransparent"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Enable Image transparency"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </isPopUpImageTransparent>
                        <pop-up-image
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            granite:class="s7-image-listener"
                            fieldLabel="Pop-up Image"
                            filter="hierarchy"
                            listOrder="8"
                            metaType="reference"
                            name="popUpImage"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/dam"
                            showEmptyInReadOnly="true"
                            showThumbnail="true"
                            valueType="string/reference">
                            <granite:data
                                selector="s7-image-listener"
                                jcr:primaryType="nt:unstructured"
                                showThumbnail="true"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </pop-up-image>
                        <pop-up-image-filereference
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Pop-up Image fileReference"
                            listOrder="21"
                            metaType="text-single"
                            name="s7popUpImage"
                            renderReadOnly="true"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </pop-up-image-filereference>
                        <latitude
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Latitude"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="lat"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </latitude>
                        <longitude
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Longitude"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="lng"
                            renderReadOnly="false"
                            required="on"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </longitude>
                        <weather-label
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Field added for future purpose. Inactive for now."
                            fieldLabel="Weather Label"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="weatherLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </weather-label>
                        <id
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="This ID will be used to fetch the destination weather from the OpenWeather API"
                            fieldLabel="Weather Id"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="id"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </id>
                        <about-heading
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="About Heading"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="aboutTitle"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </about-heading>
                        <about-description
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/admin/components/authoring/contenteditor/multieditor"
                            cfm-element="About Description"
                            checked="false"
                            default-mime-type="text/html"
                            fieldDescription="About Description"
                            listOrder="24"
                            metaType="text-multi"
                            name="aboutDescription"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string/multiline"/>
                        <hideReadMore
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="22"
                            metaType="boolean"
                            name="hideReadMore"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Hide Read More&#x9;"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </hideReadMore>
                        <readMoreLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            emptyText="Enter the read more label"
                            fieldLabel="Read More Label&#x9;"
                            listOrder="23"
                            maxlength="255"
                            metaType="text-single"
                            name="readMoreLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </readMoreLabel>
                        <readLessLabel
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            emptyText="Enter the read more label"
                            fieldLabel="Read Less Label&#x9;"
                            listOrder="23"
                            maxlength="255"
                            metaType="text-single"
                            name="readLessLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </readLessLabel>
                        <hideFavorite
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="21"
                            metaType="boolean"
                            name="hideFavorite"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Hide Favorite"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </hideFavorite>
                        <hideShareIcon
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                            listOrder="4"
                            metaType="boolean"
                            name="hideShare"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            text="Hide Share Icon"
                            valueType="boolean">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </hideShareIcon>
                        <categoriesIcon
                            cq:showOnCreate="true"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/coral/common/form/tagfield"
                            autocreateTag="true"
                            fieldLabel="Category Tags"
                            forceSelection="false"
                            listOrder="7"
                            metaType="tags"
                            multiple="true"
                            name="categories"
                            renderReadOnly="false"
                            rootPath="/content/cq:tags/sauditourism/categories"
                            showEmptyInReadOnly="true"
                            valueType="string/tags[]"/>
                        <pagePath
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Destination Page Path"
                            filter="hierarchy"
                            listOrder="21"
                            metaType="reference"
                            name="pagePath"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            rootPath="/content/sauditourism"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </pagePath>
                        <best-time-label
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Field added for future purpose. Inactive for now."
                            fieldLabel="Best Time to Visit Label"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="bestTimeLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </best-time-label>
                        <seasonal-temperatures
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                            composite="{Boolean}true"
                            fieldLabel="Seasonal temperatures"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="seasonalTemperatures"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string[]">
                            <field
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                name="./seasonalTemperatures">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <seasonId
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldDescription="Select Season"
                                                fieldLabel="Season ID"
                                                name="./seasonId"
                                                maxlength="255"
                                                renderReadOnly="false"
                                                showEmptyInReadOnly="true"
                                                valueType="string">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <winter
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Winter"
                                                        value="winter"/>
                                                    <spring
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Spring"
                                                        value="spring"/>
                                                    <summer
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Summer"
                                                        value="summer"/>
                                                    <autumn
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Autumn"
                                                        value="autumn"/>
                                                </items>
                                            </seasonId>
                                            <maxTemp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Enter Max Temperature"
                                                fieldLabel="Max Temperature"
                                                name="./maxTemp"
                                                maxlength="255"
                                                renderReadOnly="false"
                                                showEmptyInReadOnly="true"
                                                valueType="string"/>
                                            <minTemp
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                fieldDescription="Enter Min Temperature"
                                                fieldLabel="Min Temperature"
                                                name="./minTemp"
                                                maxlength="255"
                                                renderReadOnly="false"
                                                showEmptyInReadOnly="true"
                                                valueType="string"/>
                                        </items>
                                    </column>
                                </items>
                            </field>
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </seasonal-temperatures>
                        <transportation-label
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Field added for future purpose. Inactive for now."
                            fieldLabel="Transportation Label"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="transportationLabel"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </transportation-label>
                        <transportation-view-all-title
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldDescription="Field added for future purpose. Inactive for now."
                            fieldLabel="Transportation View All - Title"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="transportationViewAllTitle"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </transportation-view-all-title>
                        <transportation-view-all-link
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldDescription="Select an image to be used as a thumbnail for the video"
                            fieldLabel="Transportation View All - Link"
                            filter="hierarchy"
                            listOrder="21"
                            metaType="reference"
                            name="transportationViewAllLink"
