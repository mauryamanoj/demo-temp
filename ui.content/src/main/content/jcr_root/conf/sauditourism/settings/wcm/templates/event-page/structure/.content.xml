<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/sauditourism/settings/wcm/templates/event-page"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="sauditourism/components/structure/event-page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <menu
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/menu"
                displayMode="multi"
                fragmentPath="/content/dam/sauditourism/cf/{language}/header/menu"
                variationName="master"/>
            <header_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/header-section-event"
                editable="{Boolean}true"/>
            <gallery_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/gallery-section"
                editable="{Boolean}true"/>
            <about_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/about-section"
                editable="{Boolean}true"/>
            <information_section
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/information-section"
                editable="{Boolean}true"/>
            <rightsidebar
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                editable="{Boolean}true"/>
            <event_date_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/event-date-widget"
                editable="{Boolean}true"/>
            <price_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/price-widget"
                editable="{Boolean}true"/>
            <special_show_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/special-show-widget"
                editable="{Boolean}true"/>
            <information_list_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/information-list-widget"
                editable="{Boolean}true"/>
            <map_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/map-widget"
                editable="{Boolean}true"/>
            <help_widget
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/help-widget"
                editable="{Boolean}true"/>
            <responsivegrid
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid"
                editable="{Boolean}true"/>
            <footer-block
                jcr:primaryType="nt:unstructured"
                sling:resourceType="sauditourism/components/content/contentfragmentfooter"
                displayMode="multi"
                fragmentPath="/content/dam/sauditourism/cf/{language}/footer"
                variationName="master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
