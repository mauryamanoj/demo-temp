<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Native App Home Page Order config"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="dialog-container"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <featureSections
                        jcr:description="Native APP Feature"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Native APP Feature"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <cards
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Features">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/accordion"
                                    name="./features">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Items"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <features
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                    fieldLabel="Select Feature"
                                                    name="./item">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <maps
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Maps"
                                                            value="maps"/>
                                                        <search
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Search"
                                                            value="search"/>
                                                        <signin
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Sign In"
                                                            value="signIn"/>
                                                        <announcements
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Announcements"
                                                            value="announcements"/>
                                                        <packages
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Packages"
                                                            value="packages"/>
                                                        <deals
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Deals"
                                                            value="deals"/>
                                                        <saudicalendar
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Saudi Calendar"
                                                            value="saudiCalendar"/>
                                                        <destinations
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Destinations"
                                                            value="destinations"/>
                                                        <hotels
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Hotels"
                                                            value="hotels"/>
                                                        <activities
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Activities"
                                                            value="activities"/>
                                                        <addtofavorite_x00a0_
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Add To Favourite"
                                                            value="addToFavourite"/>
                                                        <shareitem_x00a0_
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Share Item"
                                                            value="shareItem"/>
                                                        <roadtrips_x00a0_
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Road Trips"
                                                            value="roadTrips"/>
                                                        <seeanddo
                                                            jcr:primaryType="nt:unstructured"
                                                            text="See &amp; Do"
                                                            value="seeAndDo"/>
                                                        <calendar
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Calendar"
                                                            value="calendar"/>
                                                        <loyalty
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Loyalty"
                                                            value="loyalty"/>
                                                        <essentials
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Essentials"
                                                            value="essentials"/>
                                                        <mytrip
                                                            jcr:primaryType="nt:unstructured"
                                                            text="My Trip"
                                                            value="myTrip"/>
                                                        <booking
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Booking"
                                                            value="booking"/>
                                                        <getdirections
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Get Directions"
                                                            value="getDirections"/>
                                                        <smartpass
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Smart Pass"
                                                            value="smartPass"/>
                                                        <weather
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Weather"
                                                            value="weather"/>
                                                        <showseasons
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Show Seasons"
                                                            value="showSeasons"/>
                                                        <showmapsindoor
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Show Maps Indoor"
                                                            value="showMapsIndoor"/>
                                                        <addToWallet
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Add to Wallet"
                                                            value="addToWallet"/>
                                                        <directBooking
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Direct Booking"
                                                            value="directBooking"/>
                                                        <viewProfile
                                                            jcr:primaryType="nt:unstructured"
                                                            text="View Profile"
                                                            value="viewProfile"/>
                                                        <adobeTarget
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Adobe Target"
                                                            value="adobeTarget"/>
                                                        <showProvice
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Show Province"
                                                            value="showProvince"/>
                                                        <entertainer
                                                            jcr:primaryType="nt:unstructured"
                                                            text="Entertainer"
                                                            value="entertainer"/>

                                                    </items>
                                                </features>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </cards>
                        </items>
                    </featureSections>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
