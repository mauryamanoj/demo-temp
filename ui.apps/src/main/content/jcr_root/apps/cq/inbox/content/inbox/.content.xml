<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:mixinTypes="[granite:InternalArea]"
    jcr:primaryType="cq:Page">
    <calendarview/>
    <header/>
    <addtask/>
    <jcr:content
        jcr:mixinTypes="[sling:VanityPath]"
        jcr:primaryType="nt:unstructured"
        jcr:title="Inbox"
        sling:redirect="{Boolean}false"
        sling:resourceType="cq/inbox/gui/components/inbox/collectionpage"
        sling:vanityOrder="{Long}301"
        sling:vanityPath="/aem/inbox"
        consoleId="cq-workflow-inbox"
        contentPath="/etc/workflow/instances"
        currentView="${state[&quot;shell.collectionpage.layoutId&quot;].string}"
        modeGroup="cq-workflow-inbox-list"
        omnisearchLocationPath="/libs/granite/omnisearch/content/metadata/inbox"
        targetCollection=".cq-workflow-inbox-list">
        <head/>
        <breadcrumbs/>
        <views/>
        <actions jcr:primaryType="nt:unstructured">
            <secondary/>
            <selection jcr:primaryType="nt:unstructured">
                <completeTask/>
                <completeWorkitem/>
                <delegateWorkitem/>
                <openProject/>
                <openPayload/>
                <createProjectFromTask/>
                <approveTask/>
                <rejectTask/>
                <claim/>
                <return/>
                <shareinboxitem/>
                <stepbackWorkitem
                    granite:rel="cq-inbox-workitem-stepback"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/actiondd"
                    action="cq.inbox.action.stepbackWorkitem"
                    activeSelectionCount="single"
                    icon="undo"
                    text="Step Back"
                    variant="actionBar"/>
              <reassignTask/>
              <delegateWorkitem
                  granite:rel="cq-inbox-workitem-delegate"
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/collection/actiondd"
                  action="cq.inbox.action.delegateWorkitem"
                  activeSelectionCount="single"
                  icon="delegate"
                  text="Delegate"
                  variant="actionBar"/>
              <details
                  granite:rel="cq-inbox-task-details"
                  jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                  action="cq.inbox.opendetails"
                  activeSelectionCount="single"
                  icon="edit"
                  text="Show Comments"
                  title="Show Comments"
                  variant="actionBar">
                <data
                    jcr:primaryType="nt:unstructured"
                    href.uritemplate="/mnt/overlay/cq/inbox/content/inbox/details/task.html{?item}&amp;_charset_=utf-8"/>
              </details>
                <openversion
                    granite:rel="cq-inbox-open-payload"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/collection/action"
                    action="cq.inbox.openversion"
                    activeSelectionCount="single"
                    icon="check"
                    text="Compare with"
                    title="Compare"
                    variant="actionBar">
                    <data
                        jcr:primaryType="nt:unstructured"
                        linkAttributeName="payloadLink"
                        target="_blank"/>
                </openversion>
            </selection>
        </actions>
        <rails/>
    </jcr:content>
    <images/>
    <details/>
    <dialogs/>
    <commons/>
</jcr:root>
